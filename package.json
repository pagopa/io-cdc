{
  "name": "monorepo",
  "private": true,
  "scripts": {
    "code-review": "turbo run format:check lint:check test:coverage",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "test": "turbo run test",
    "test:coverage": "turbo run test:coverage",
    "build": "turbo run build",
    "typecheck": "turbo run typecheck",
    "version": "changeset version && pnpm install",
    "release": "changeset tag",
    "generate": "turbo run generate",
    "frontend:dev": "pnpm --filter frontend dev",
    "frontend:build": "pnpm --filter frontend build",
    "frontend:preview": "pnpm --filter frontend preview --host",
    "ui:build": "pnpm --filter @io-cdc/ui build",
    "mixpanel:build": "pnpm --filter @io-cdc/mixpanel build"
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@changesets/cli": "^2.27.1",
    "openapi-zod-client": "^1.18.2",
    "openapi3-ts": "3.1.0",
    "prettier": "3.1.1",
    "turbo": "^2.0.7",
    "typescript": "^5.8.3",
    "vite": "^5.4.1",
    "vite-plugin-dts": "^4.2.3",
    "vite-plugin-externalize-deps": "^0.8.0"
  },
  "resolutions": {
    "@azure/core-client": "1.9.0",
    "@azure/core-http-compat": "2.3.0",
    "@azure/storage-common": "12.1.0"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a"
}
